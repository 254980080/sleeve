.container {
  width: 100%;
  height: 320rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.inner-container {
  width: 730rpx;
  height: 300rpx;
  border: 1px dashed #dcebe6;
}
.grid-item {
  width: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.text {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #333;
}
.img {
  width: 60rpx;
  height: 60rpx;
}